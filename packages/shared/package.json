{
  "name": "@octopoid/shared",
  "version": "2.0.0",
  "description": "Shared TypeScript types for Octopoid",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "octopoid",
    "types"
  ],
  "author": "Octopoid Contributors",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.3.3"
  }
}
